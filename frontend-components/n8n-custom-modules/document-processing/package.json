{
  "name": "@n8n/document-processing-module",
  "version": "2.0.0",
  "description": "Production-ready document processing module for n8n with complete validation, error handling, authentication, health checks, and backup/recovery",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "test": "jest --config jest.config.js --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "autocannon -c 100 -d 30 http://localhost:3000/health",
    "setup": "node setup.js",
    "validate-config": "node validate-config.js",
    "setup:prod": "NODE_ENV=production node setup.js",
    "health-check": "node scripts/health-check.js",
    "backup-create": "node scripts/backup-create.js",
    "backup-restore": "node scripts/backup-restore.js",
    "backup-list": "node scripts/backup-list.js",
    "audit-report": "node scripts/audit-report.js",
    "audit-cleanup": "node scripts/audit-cleanup.js",
    "db-status": "node scripts/db-status.js",
    "db-init": "node scripts/db-init.js",
    "import-workflows": "node import-workflows.js",
    "export-workflows": "node export-workflows.js",
    "deploy:docker": "docker build -t n8n-document-processing:latest . && docker push n8n-document-processing:latest",
    "deploy:docker:dev": "docker-compose up -d",
    "deploy:docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "deploy:k8s": "kubectl apply -f k8s-deployment.yaml",
    "undeploy:k8s": "kubectl delete -f k8s-deployment.yaml",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "generate-keys": "node scripts/generate-keys.js",
    "rotate-keys": "node scripts/rotate-keys.js"
  },
  "keywords": [
    "n8n",
    "document-processing",
    "workflow",
    "automation",
    "api",
    "pdf",
    "invoice",
    "validation",
    "authentication",
    "production"
  ],
  "author": "Your Organization",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourorg/n8n-document-processing.git"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.9.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "optionalDependencies": {
    "crypto-js": "^4.1.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "googleapis": "^118.0.0",
    "joi": "^17.9.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.7",
    "openai": "^4.26.0",
    "pdfkit": "^0.13.0",
    "prom-client": "^14.2.0",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "autocannon": "^7.10.0",
    "concurrently": "^9.2.1",
    "electron": "^38.4.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "mock-require": "^3.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "wait-on": "^9.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "mongodb": ">=5.0.0",
    "redis": ">=6.0.0"
  },
  "engines-strict": true,
  "preferGlobal": false,
  "bin": {}
}
