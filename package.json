{
  "name": "saudi-business-gate-enterprise",
  "version": "2.0.0",
  "description": "Saudi Business Gate Enterprise - The 1st Autonomous Business Gate in the Region from Saudi Arabia to the World ðŸ‡¸ðŸ‡¦ - AI-Powered Enterprise Business Management Platform",
  "type": "module",
  "workspaces": [
    "apps",
    "uikit"
  ],
  "scripts": {
    "dev": "next dev -p 3051",
    "dev:auto": "node scripts/start-dev.js",
    "dev:safe": "next dev --port 0",
    "ws": "node scripts/start-websocket.js",
    "ws:dev": "ts-node lib/websocket-server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run ws:dev\"",
    "build": "prisma generate && next build",
    "start": "next start -p 3051",
    "postinstall": "echo 'Skipping Prisma generate in postinstall (will run during build)'",
    "start:auto": "next start --port 0",
    "lint": "next lint",
    "check-ports": "node -e \"console.log('Checking ports...'); require('./scripts/start-dev.js').findAvailablePort().then(p => console.log('Available port:', p))\"",
    "deploy:all": "node scripts/deploy-all.js",
    "deploy:build": "node scripts/deploy-all.js --build",
    "generate:files": "node scripts/generate-missing-files.js",
    "validate:api": "node scripts/validate-api-ui-connections.js",
    "validate:api:watch": "nodemon scripts/validate-api-ui-connections.js",
    "validate:api:ci": "node scripts/validate-api-ui-connections.js && exit 0",
    "validate:report": "open api-ui-validation-report.html",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:ci": "jest --config jest.config.cjs --ci --coverage --watchAll=false",
    "test:api": "jest tests/api --config jest.config.cjs",
    "test:lib": "jest tests/lib --config jest.config.cjs",
    "test:crud": "node scripts/test-crud.js",
    "test:integration": "jest tests/integration --config jest.config.cjs",
    "validate:production": "node --loader ts-node/esm scripts/validate-production.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:auth": "jest __tests__/auth.test.ts",
    "test:security": "jest __tests__/security.test.ts",
    "test:load": "jest __tests__/load.test.ts",
    "db:test:setup": "NODE_ENV=test npx prisma db push --force-reset",
    "db:test:migrate": "NODE_ENV=test npx prisma migrate deploy",
    "db:test:seed": "NODE_ENV=test node __tests__/scripts/seed-test-data.js",
    "db:test:cleanup": "NODE_ENV=test node __tests__/scripts/cleanup-test-data.js",
    "lint:test": "eslint '__tests__/**/*.{js,jsx,ts,tsx}'",
    "format:test": "prettier --write '__tests__/**/*.{js,jsx,ts,tsx}'",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "i18n:watch": "lingui extract --watch",
    "apps:dev": "npm --workspace apps run dev",
    "apps:build": "npm --workspace apps run build",
    "apps:start": "npm --workspace apps run start",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts",
    "db:seed:finance": "ts-node --esm prisma/seed-finance.ts",
    "db:seed:complete": "node --loader ts-node/esm prisma/seed-complete.ts",
    "db:seed:modules": "node --loader ts-node/esm prisma/seed-all-modules.ts",
    "db:seed:all": "npm run db:seed:complete && npm run db:seed:modules",
    "db:setup": "prisma generate && prisma db push",
    "db:setup:full": "node scripts/run-database-setup.js",
    "db:configure": "node scripts/configure-and-run-db.js",
    "db:reset": "prisma migrate reset --force",
    "db:test": "node scripts/test-db-connection.js",
    "db:check": "node scripts/check-database.js",
    "verify:integration": "node scripts/verify-integration.js",
    "verify:production": "node scripts/verify-production-integrations.js",
    "verify:all": "npm run verify:integration && npm run verify:production",
    "setup:security": "node scripts/setup-production-security.js",
    "setup:production": "node scripts/complete-production-setup.js",
    "db:production:setup": "node scripts/setup-production-db.js",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:sql": "node scripts/run-database-setup.js",
    "db:studio": "prisma studio",
    "deploy:vercel": "scripts/deploy-to-vercel.bat",
    "refactor": "node scripts/refactor-project.js",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit"
  },
